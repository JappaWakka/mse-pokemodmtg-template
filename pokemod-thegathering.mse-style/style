mse version: 2.0.0
game: pokemod
short name: PokémodMTG
full name: Pokémod the Gathering
installer group: pokemod/template style
version: 1.0
icon: pokemonmtg-icon.png

depends on: pokemod.mse-game 1.0.0
depends on: pokemod-thegathering-coloredsymbols.mse-symbol-font

card background: rgb(0,0,0)
card width: 600
card height: 825
card dpi: 200

init script:
	#### Variables ####
	attack_rule_width_withdamage := { 366 }
	attack_rule_width_nodamage := { 434 }
	attack_damage_width := { 73 }
	attack_cost_left := { 40 }
	attack_cost_width := { 73 }
	
	text_area_top := { 470 }
	text_area_height := { 210 }
	text_area_fields_x_offset := { 8 }
	text_area_fields_y_offset := { 4 }
		

	### 2 Attacks/Powers ###
	input_2_attack_line_max_1 := { 18 }
	input_2_attack_line_max_2 := { 60 }
	input_2_attack_line_max_3 := { 91 }
	input_2_attack_line_max_4 := { 120 }
	
	input_2_power_line_max_2 := { 118 }
	
	### 3 Attacks/Powers ###
	
	input_3_attack_line_max_1 := { 37 }
	input_3_attack_line_max_2 := { 93 }
	input_3_attack_line_max_3 := { 191 }
	input_3_power_line_max_1 := { 64 }
	
	is_pkmn_power := {if set.language == "English" then if contains(value, match: "mon Power:") or contains(value, match: "-Power:") then "true" else "false" else if set.language == "Nederlands" then if contains(value, match: "-Kracht:") then "true" else "false" else "false"}
	is_pkmn_body := {if set.language == "English" then if contains(value, match: "mon Body:") or contains(value, match: "-Body:") then "true" else "false" else if set.language == "Nederlands" then if contains(value, match: "-Eigenschap:") then "true" else "false" else "false"}
	is_baby_pkmn := {if set.language == "English" then if contains(value, match: "Baby Pok") then "true" else "false" else if set.language == "Nederlands" then if contains(value, match: "Baby-Pok") then "true" else "false" else "false"}
	ability_color_filter := { value } +
		remove_tag@(tag: "<color") +
		replace@(match: "<b>", replace: { if is_pkmn_power(value) or is_baby_pkmn(value) then "<b><color:rgb(163,9,11)>" else if is_pkmn_body(value) then "<b><color:rgb(17,83,17)>" else "<b>" } ) +
		replace@(match: "</b>", replace: { if not(is_pkmn_power(value)) and not(is_pkmn_body(value)) and not(is_baby_pkmn(value)) then "</b>" else "</b></color>" })

	
	#### Left position of the Attacks/Powers ####
	
	power_left :=
		{
			if card.card_type_select == "Pokemon"
				then
				if card.pokemon_attackpower_combination == "2 Powers" or card.pokemon_attackpower_combination == "1 Power 1 Attack" or card.pokemon_attackpower_combination == "1 Power 0 Attacks" or card.pokemon_attackpower_combination == "1 Power 2 Attacks"
					then 40
					else 0
				else 0
		}
	attack_rule_left :=
		{
			if card.card_type_select == "Pokemon"
				then
				if card.pokemon_attackpower_combination == "1 Attack" or card.pokemon_attackpower_combination == "2 Attacks" or card.pokemon_attackpower_combination == "3 Attacks" or card.pokemon_attackpower_combination == "1 Power 1 Attack" or card.pokemon_attackpower_combination == "1 Power 2 Attacks"
					then attack_cost_left() + attack_cost_width() + text_area_fields_x_offset()
					else 0
				else
				if card.trainer_type_select == "Stadium with Attack" or card.trainer_type_select == "Technical Machine" or card.trainer_type_select == "G-SPEC with Attack"
					then attack_cost_left() + attack_cost_width() + text_area_fields_x_offset()
				else 0
		}
	
	
	attack1_damage_left :=
		{
			if card.card_type_select == "Pokemon"
				then
				if card.attack1_damage_exist == "yes"
					then attack_rule_left() + attack_rule_width_withdamage() + text_area_fields_y_offset()
					else 0
				else 0
		}
	

	attack2_damage_left :=
		{
			if card.card_type_select == "Pokemon"
				then
				if card.attack2_damage_exist == "yes"
					then attack_rule_left() + attack_rule_width_withdamage() + text_area_fields_y_offset()
					else 0
				else 0
		}
	
	attack3_damage_left :=
		{
			if card.card_type_select == "Pokemon"
				then
				if card.attack3_damage_exist == "yes"
					then attack_rule_left() + attack_rule_width_withdamage() + text_area_fields_y_offset()
					else 0
				else 0
		}


	#### Top position of the Attacks/Powers ####
	
	power1_top :=
	{
		if card.card_type_select == "Pokemon"
			then
			if card.pokemon_attackpower_combination == "2 Powers" or card.pokemon_attackpower_combination == "1 Power 1 Attack" or card.pokemon_attackpower_combination == "1 Power 0 Attacks" or card.pokemon_attackpower_combination == "1 Power 2 Attacks"
				then text_area_top()
			else 0
		else 0
	}
	power2_top :=
	{
		if card.card_type_select == "Pokemon"
			then
			if card.pokemon_attackpower_combination == "2 Powers"
				then power1_top() + power1_height() + text_area_fields_y_offset()
				else 0
			else 0
	}
	
	attack1_top :=
	{
		if card.card_type_select == "Pokemon"
			then
			if card.pokemon_attackpower_combination == "1 Power 1 Attack"
				then
				if card.attack1_damage_exist == "yes"
					then
					if length(remove_tags(card.power1_rules)) > input_2_power_line_max_2()
						then
						if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
							then text_area_top() + text_area_height() / 5 * 3 + text_area_fields_y_offset()
							else text_area_top() + text_area_height() / 2 + text_area_fields_y_offset()
						else
						if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
							then text_area_top() + text_area_height() / 2 + text_area_fields_y_offset()
							else text_area_top() + text_area_height() / 5 * 2 - 8 + text_area_fields_y_offset()
					else
						if length(remove_tags(card.power1_rules)) > input_2_power_line_max_2()
							then
							if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
								then text_area_top() + text_area_height() / 5 * 3 + text_area_fields_y_offset()
								else text_area_top() + text_area_height() / 2 + text_area_fields_y_offset()
							else
							if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
								then text_area_top() + text_area_height() / 2 + text_area_fields_y_offset()
								else text_area_top() + text_area_height() / 5 * 2 - 8 + text_area_fields_y_offset()
				else
				if card.pokemon_attackpower_combination == "1 Power 2 Attacks"
					then power1_top() + power1_height() + text_area_fields_y_offset()
					else
					if card.pokemon_attackpower_combination == "1 Power 0 Attacks"
						then 0
						else text_area_top()
			else
			if card.card_type_select == "Trainer"
				then
				if card.trainer_type_select == "Stadium with Attack" or card.trainer_type_select == "Technical Machine" or card.trainer_type_select == "G-SPEC with Attack"
					then 474 + card_style.trainer_rules.height + text_area_fields_y_offset()
					else 0
				else 0
	}
				
	attack2_top :=
		{
			if card.card_type_select == "Pokemon"
				then
				if card.pokemon_attackpower_combination == "2 Attacks" or card.pokemon_attackpower_combination == "3 Attacks" or card.pokemon_attackpower_combination == "1 Power 2 Attacks"
					then attack1_top() + attack1_height() + text_area_fields_y_offset()
					else 0
				else 0
		}
		
	attack3_top :=
		{
			if card.card_type_select == "Pokemon"
				then
				if card.pokemon_attackpower_combination == "3 Attacks"
					then attack2_top() + attack2_height() + text_area_fields_y_offset()
					else 0
				else 0
		}
	
	#### Width of the Attacks/Powers ####
	
	power1_width :=
	{
		if card.card_type_select == "Pokemon"
			then
			if card.pokemon_attackpower_combination == "2 Powers" or card.pokemon_attackpower_combination == "1 Power 1 Attack" or card.pokemon_attackpower_combination == "1 Power 0 Attacks" or card.pokemon_attackpower_combination == "1 Power 2 Attacks"
				then 523
				else 0
			else 0
	}

	#### Height of the Attacks/Powers ####

	power1_height :=
		{
			if card.card_type_select == "Pokemon"
				then
				if card.pokemon_attackpower_combination == "1 Power 0 Attacks"
					then text_area_height()
					else
					if card.pokemon_attackpower_combination == "2 Powers"
						then if is_baby_pkmn(card.power1_rules)
							then
								text_area_height() / 5 * 2 - 4
							else
								if length(remove_tags(card.power1_rules)) > input_2_power_line_max_2()
									then
										if length(remove_tags(card.power2_rules)) <= input_2_attack_line_max_3()
											then text_area_height() / 5 * 3
											else text_area_height() / 2
									else
										if length(remove_tags(card.power2_rules)) <= input_2_attack_line_max_3()
											then text_area_height() / 2
											else text_area_height() / 5 * 2 - 8
						else
						if card.pokemon_attackpower_combination == "1 Power 1 Attack"
							then
							if card.attack1_damage_exist == "yes"
								then
								if length(remove_tags(card.power1_rules)) > input_2_power_line_max_2()
									then
									if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
										then text_area_height() / 5 * 3
										else text_area_height() / 2
									else
									if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
										then text_area_height() / 2
										else text_area_height() / 5 * 2 - 8
								else
								if length(remove_tags(card.power1_rules)) > input_2_power_line_max_2()
									then
									if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
										then text_area_height() / 5 * 3
										else text_area_height() / 2
									else
									if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
										then text_area_height() / 2
										else text_area_height() / 5 * 2 - 8
							else
							if card.pokemon_attackpower_combination == "1 Power 2 Attacks"
									then
									if length(remove_tags(card.power1_rules)) > input_3_power_line_max_1()
										then
										if length(remove_tags(card.attack1_rules)) > input_3_attack_line_max_2()
											then
											if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
												then text_area_height() / 3 - 2
												else text_area_height() / 3 - 2 + 12
											else
											if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
												then text_area_height() / 3 - 2 + 12
												else text_area_height() / 3 - 2 + 28
										else
										if length(remove_tags(card.attack1_rules)) > input_3_attack_line_max_2()
											then
											if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
												then text_area_height() / 3 - 2 - 28
												else text_area_height() / 3 - 2 - 16
											else
											if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
												then text_area_height() / 3 - 2 - 16
												else text_area_height() / 3 - 2
									else 0
								else 0
		}
	power2_height := 
	{
		if card.card_type_select == "Pokemon"
			then
			if card.pokemon_attackpower_combination == "2 Powers"
				then text_area_height() - power1_height() - 4
				else 0
			else 0
	}
		
	attack1_height :=
		{
			if card.card_type_select == "Pokemon"
				then
				if card.pokemon_attackpower_combination == "1 Power 1 Attack"
					then
					if card.attack1_damage_exist == "yes"
						then
						if length(remove_tags(card.power1_rules)) > input_2_power_line_max_2()
							then
								if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
									then text_area_height() / 5 * 2 - 4
									else text_area_height() / 2 - 4
								else
								if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
									then text_area_height() / 2 - 4
									else text_area_height() / 5 * 3 + 4
						else
						if length(remove_tags(card.power1_rules)) > input_2_power_line_max_2()
							then
								if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
									then text_area_height() / 5 * 2 - 4
									else text_area_height() / 2 - 4
								else
								if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
									then text_area_height() / 2 - 4
									else text_area_height() / 5 * 3 + 4
					else
					if card.pokemon_attackpower_combination == "1 Attack"
						then text_area_height()
						else
						if card.pokemon_attackpower_combination == "2 Attacks"
							then
							if card.attack1_damage_exist == "yes"
								then
								if card.attack2_damage_exist == "yes"
									then
									if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
										then
										if length(remove_tags(card.attack2_rules)) <= input_2_attack_line_max_3()
											then text_area_height() / 2
											else text_area_height() / 5 * 2
										else
										if length(remove_tags(card.attack2_rules)) <= input_2_attack_line_max_3()
											then text_area_height() / 5 * 3
											else text_area_height() / 2
									else
									if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3()
										then
										if length(remove_tags(card.attack2_rules)) <= input_2_attack_line_max_3() + 31
											then text_area_height() / 2
											else text_area_height() / 5 * 2
										else
										if length(remove_tags(card.attack2_rules)) <= input_2_attack_line_max_3() + 31
											then text_area_height() / 5 * 3
											else text_area_height() / 2
								else
								if card.attack2_damage_exist == "yes"
									then
									if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3() + 31
										then
										if length(remove_tags(card.attack2_rules)) <= input_2_attack_line_max_3()
											then text_area_height() / 2
											else text_area_height() / 5 * 2
										else
										if length(remove_tags(card.attack2_rules)) <= input_2_attack_line_max_3()
											then text_area_height() / 5 * 3
											else text_area_height() / 2
									else
									if length(remove_tags(card.attack1_rules)) <= input_2_attack_line_max_3() + 31
										then
										if length(remove_tags(card.attack2_rules)) <= input_2_attack_line_max_3() + 31
											then text_area_height() / 2
											else text_area_height() / 5 * 2
										else
										if length(remove_tags(card.attack2_rules)) <= input_2_attack_line_max_3() + 31
											then text_area_height() / 5 * 3
											else text_area_height() / 2
							else
							if card.pokemon_attackpower_combination == "3 Attacks"
								then
								if length(remove_tags(card.attack1_rules)) > input_3_attack_line_max_2()
									then
									if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
										then
										if length(remove_tags(card.attack3_rules)) > input_3_attack_line_max_2()
											then text_area_height() /3-2
											else text_area_height() /3-2+12
										else
											if length(remove_tags(card.attack3_rules)) > input_3_attack_line_max_2()
												then text_area_height() /3-2+12
												else text_area_height() /3-2+28
									else
									if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
											then
											if length(remove_tags(card.attack3_rules)) > input_3_attack_line_max_2()
												then text_area_height() /3-2-28
												else text_area_height() /3-2-16
											else
											if length(remove_tags(card.attack3_rules)) > input_3_attack_line_max_2()
												then text_area_height() /3-2-16
												else text_area_height() /3-2
								else
								if card.pokemon_attackpower_combination == "1 Power 2 Attacks"
									then
									if length(remove_tags(card.attack1_rules)) > input_3_attack_line_max_2()
										then
										if length(remove_tags(card.power1_rules)) > input_3_power_line_max_1()
											then
											if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
												then text_area_height() /3-2
												else text_area_height() /3-2+12
											else
											if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
												then text_area_height() /3-2+12
												else text_area_height() /3-2+28
										else
										if length(remove_tags(card.power1_rules)) > input_3_power_line_max_1()
											then
											if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
												then text_area_height() /3-2-28
												else text_area_height() /3-2-16
											else
											if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
												then text_area_height() /3-2-16
												else text_area_height() /3-2
									else 0
				else
				if card.card_type_select == "Trainer"
					then
					if card.trainer_type_select == "Stadium with Attack" or card.trainer_type_select == "Technical Machine" or card.trainer_type_select == "G-SPEC with Attack"
						then text_area_height() / 5 * 3
						else 0
					else 0
		}
		
	attack2_height :=
		{
			if card.card_type_select == "Pokemon"
				then
				if card.pokemon_attackpower_combination == "2 Attacks"
					then text_area_height() - attack1_height() - 4
					else
					if card.pokemon_attackpower_combination == "3 Attacks"
						then
						if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
							then
							if length(remove_tags(card.attack1_rules)) > input_3_attack_line_max_2()
								then
								if length(remove_tags(card.attack3_rules)) > input_3_attack_line_max_2()
									then text_area_height() /3-2
									else text_area_height() /3-2+12
								else
								if length(remove_tags(card.attack3_rules)) > input_3_attack_line_max_2()
									then text_area_height() /3-2+12
									else text_area_height() /3-2+28
							else
							if length(remove_tags(card.attack1_rules)) > input_3_attack_line_max_2()
								then
								if length(remove_tags(card.attack3_rules)) > input_3_attack_line_max_2()
									then text_area_height() /3-2-28
									else text_area_height() /3-2-16
								else
								if length(remove_tags(card.attack3_rules)) > input_3_attack_line_max_2()
									then text_area_height() /3-2-16
									else text_area_height() /3-2
						else
						if card.pokemon_attackpower_combination == "1 Power 2 Attacks"
							then
							if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
								then
								if length(remove_tags(card.power1_rules)) > input_3_power_line_max_1()
									then
									if length(remove_tags(card.attack1_rules)) > input_3_attack_line_max_2()
										then text_area_height() /3-2
										else text_area_height() /3-2+12
									else
									if length(remove_tags(card.attack1_rules)) > input_3_attack_line_max_2()
										then text_area_height() /3-2+12
										else text_area_height() /3-2+28
								else
								if length(remove_tags(card.power1_rules)) > input_3_power_line_max_1()
											then
											if length(remove_tags(card.attack1_rules)) > input_3_attack_line_max_2()
												then text_area_height() /3-2-28
												else text_area_height() /3-2-16
											else
											if length(remove_tags(card.attack1_rules)) > input_3_attack_line_max_2()
												then text_area_height() /3-2-16
												else text_area_height() /3-2
				else 0
			else 0
		}
		
	attack3_height :=
	{
		if card.card_type_select == "Pokemon"
			then
			if card.pokemon_attackpower_combination == "3 Attacks"
				then
				if length(remove_tags(card.attack3_rules)) > input_3_attack_line_max_2()
					then
					if length(remove_tags(card.attack1_rules)) > input_3_attack_line_max_2()
						then
						if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
							then text_area_height() /3-2
							else text_area_height() /3-2+12
						else
						if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
							then text_area_height() /3-2+12
