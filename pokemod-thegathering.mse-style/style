					else
					if length(remove_tags(card.attack1_rules)) > input_3_attack_line_max_2()
						then
						if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
							then text_area_height() /3-2-28
							else text_area_height() /3-2-16
						else
						if length(remove_tags(card.attack2_rules)) > input_3_attack_line_max_2()
							then text_area_height() /3-2-16
							else text_area_height() /3-2
				else 0
			else 0
	}
	trainer_height := 
	{
		if card.card_type_select == "Trainer"
			then
			if card.trainer_type_select == "Stadium with Attack" or card.trainer_type_select == "Technical Machine" or card.trainer_type_select == "G-SPEC with Attack"
				then
				235 / 5 * 2
				else
				if card.trainer_type_select == "With HP"
					then "283"
					else 
					if card.trainer_type_select != "Regular"
						then "230"
						else "280"
